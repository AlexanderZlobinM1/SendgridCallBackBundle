{
    "name": "azlobin/mautic-sendgrid-callback",
    "description": "Mautic plugin to process SendGrid Event Webhook callbacks for bounce and complaint handling.",
    "type": "mautic-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Zlobin"
        }
    ],
    "keywords": [
        "mautic",
        "plugin",
        "sendgrid",
        "webhook"
    ],
    "require": {
        "php": ">=8.1",
        "mautic/core-lib": "^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "MauticPlugin\\SendgridCallbackBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MauticPlugin\\SendgridCallbackBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "install-directory-name": "SendgridCallbackBundle"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
